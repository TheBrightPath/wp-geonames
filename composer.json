{
  "name": "metaworx/wp-geonames",
  "version": "2.0.3",
  "description": "WP Geonames Plugin",
  "license": "GPL-3.0",
  "repositories": [
    {
      "type": "github",
      "url": "git@github.com:metaworx/geonames-client.git"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "aternus/geonames-client": "^1.0",
    "ext-mysqli": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "WPGeonames\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "classmap-authoritative": true
  }
}
